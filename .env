VITE_PUBLIC_BUILDER_KEY=__BUILDER_PUBLIC_KEY__
PING_MESSAGE="ping pong"
# GEMINI_API_KEY should be set securely in your deployment environment, not here
GEMINI_API_KEY=your-gemini-api-key-here

